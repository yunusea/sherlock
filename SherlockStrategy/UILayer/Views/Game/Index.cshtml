
<div class="col-md-12">
    <div class="left col-md-6">
        <h1>Oyunlar</h1>

    </div>
    <div class="right col-md-6 text-right">
        <input type="submit" class="btn btn-default" ng-click="GotoOngoingGameList()" value="Devam Eden Oyunlar" />
    </div>
<div class="clearfix"></div>
    <table class="table table-hover">
        <tr>
            <td>Oyun Adı</td>
            <td>İşlem</td>
        </tr>
        <tr ng-repeat="game in GetGameList">
            <td>{{game.GameName}}</td>
            <td><input type="submit" class="btn btn-default" ng-click="GotoGame(game.Id)" value="Yeni Oyun" /></td>
        </tr>
    </table>

</div>